<?xml version="1.0" ?>
<launch>
  <arg name="robot_ip" />
  <arg name="robot" default="panda" doc="choose your robot. Possible values: [panda, fr3]"/>
  <arg name="arm_id" default="$(arg robot)" />
  <arg name="device_name" default="qbhand2m1" />

  <arg name="load_gripper" default="true" />
  <arg name="xacro_args" default="" />

<!-- Load the robot description, controllers, and simulation params -->
  <param name="robot_description"
         command="xacro $(find panda_qb_description)/urdf/only_panda_description.urdf.xacro
                  gazebo:=false"
         unless="$(arg load_gripper)">
  </param>

  <param name="robot_description"
         command="xacro $(find panda_qb_description)/urdf/pandaqb_description.urdf.xacro
                  gazebo:=false"
         if="$(arg load_gripper)">
  </param>

 <!-- Launch the franka control node -->
  <node name="franka_control" pkg="franka_control" type="franka_control_node" output="screen" required="true">
    <rosparam command="load" file="$(find franka_control)/config/franka_control_node.yaml" subst_value="true" />
    <param name="robot_ip" value="$(arg robot_ip)" />
  </node>

  <!-- Launch the qb hand control node and communication handler -->
  <!-- <include file="$(find qb_hand_control)/launch/control_qbhand2m.launch">
    <arg name="activate_on_initialization" value="true"/>
    <arg name="standalone" value="true"/>
  </include> -->

  <rosparam command="load" file="$(find franka_control)/config/default_controllers.yaml" subst_value="true" />
  <rosparam command="load" file="$(find qb_hand_control)/config/qbhand2m_controllers.yaml" subst_value="true"/>

  <node name="state_controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" 
    args="franka_state_controller effort_joint_trajectory_controller"/>

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen"/>
  <node name="joint_state_publisher" type="joint_state_publisher" pkg="joint_state_publisher" output="screen">
    <!-- <rosparam if="$(arg load_gripper)" param="source_list">[franka_state_controller/joint_states, franka_gripper/joint_states] </rosparam> -->
    <rosparam param="source_list">
      [franka_state_controller/joint_states]
    </rosparam> <!--unless="$(arg load_gripper)" -->
    <param name="rate" value="30"/>
  </node>
</launch>
